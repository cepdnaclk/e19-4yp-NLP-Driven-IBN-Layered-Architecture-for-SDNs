{
  "intent": "<Sample Intent in Natural Language>", 
  "config": {
    "intent_id": "<INTENT_ID>",
    "user_role": "<USER_ROLE>", // e.g., "admin", "user", "guest"
    "timestamp": "<TIMESTAMP>",
    "QOS": {
      "application": "<APPLICATION>", // e.g.,  "VOIP", "ZOOM", "YouTube", "Netflix", "Web Browsing"
      "category": "<TRAFFIC_CATEGORY>", // e.g., "video", "voice", "web", "file"   
      "latency": "<LATENCY_THRESHOLD>",
      "bandwidth": "<BANDWIDTH_THRESHOLD>",
      "jitter": "<JITTER_THRESHOLD>", 
      "priority": "<PRIORITY_LEVEL>", // e.g., "high", "medium", "low" ??
      "time_constraints": {
        "start": "<START_TIME>",
        "end": "<END_TIME>",
        "days": ["<DAY_1>", "<DAY_2>", "<DAY_3>"] // e.g., "Monday", "Tuesday", "Wednesday"
      }
    },
    "ACL": {
      "allow_rules": [
        {
          "action": "<ACTION>", // e.g., "allow", "deny"
          "source": "<ALLOW_SOURCE_IP>", 
          "destination": "<ALLOW_DESTINATION_IP>",
          "ports": ["<ALLOW_PORT_1>", "<ALLOW_PORT_2>"], // e.g., "80", "443"
          "protocols": ["<ALLOW_PROTOCOL_1>", "<ALLOW_PROTOCOL_2>"] // e.g., "HTTP", "HTTPS"
        }
      ],
      "schedule": {
        "start": "<ACL_START_TIME>", // e.g., "2023-09-01T00:00:00Z"
        "end": "<ACL_END_TIME>" // e.g., "2023-10-01T00:00:00Z"
      }
    },
    "LOGS": {
      "filters": [
        {
          "hosts": "<HOST_IP>", // e.g., "192.168.1.1"
          "ports": "<FILTER_PORT>", // e.g., 80, 443, 8080
          "application": "<FILTER_APPLICATION>", // e.g., "WEB", "VIDEO", "VOICE", "ZOOM" | OR DSCP Value ??
          "time_window": "<TIME_WINDOW>" // e.g., "6h" ->last_6_hours, "30s" ->last_30_seconds
        }
      ]
    }
  }
}
