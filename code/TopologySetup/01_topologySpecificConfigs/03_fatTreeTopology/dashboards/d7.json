{
  "title": "Host 7 Probes to All Hosts",
  "panels": [
    {
      "type": "timeseries",
      "title": "RTT Time (d7-active-metrics)",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
      "targets": [
        {
          "expr": "probe_icmp_duration_seconds{phase=\"rtt\",job=\"d7-active-metrics\"}",
          "legendFormat": "{{instance}}"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Probe Success Rate (d7-active-metrics)",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
      "targets": [
        {
          "expr": "probe_success{job=\"d7-active-metrics\"}",
          "legendFormat": "{{instance}}"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "DNS Lookup Time (d7-active-metrics)",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
      "targets": [
        {
          "expr": "probe_dns_lookup_time_seconds{job=\"d7-active-metrics\"}",
          "legendFormat": "{{instance}}"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Total Probe Duration (d7-active-metrics)",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8},
      "targets": [
        {
          "expr": "probe_duration_seconds{job=\"d7-active-metrics\"}",
          "legendFormat": "{{instance}}"
        }
      ]
    }
  ],
  "timezone": "browser",
  "schemaVersion": 27,
  "version": 1,
  "refresh": "30s"
}
